{
	"name": "portfolio_eleventy",
	"version": "1.3.8",
	"description": "A starter template for the TEA stack.",
	"engines": {
		"node": ">=14.16.0"
	},
	"main": ".eleventy.js",
	"scripts": {
		"build": "cross-env NODE_ENV=production npm-run-all clear:* request:* build:*",
		"build:eleventy": "eleventy",
		"build:webpack": "webpack",
		"request:load": "node ./src/_requests/load.js",
		"request:profiles": "node ./src/_requests/profiles.js",
		"clear:basis": "rm -rf dist && rm -rf ./src/_data/*.json",
		"clear:js": "rm -rf ./src/_data/repos.js && rm -rf ./src/_data/articles.js && rm -rf ./src/_data/projects.js && rm -rf ./src/_data/sides.js",
		"dev": "cross-env NODE_ENV=development  npm-run-all clear:* request:* build:eleventy build:webpack -p dev:*",
		"dev:eleventy": "eleventy --serve --quiet",
		"dev:webpack": "webpack --watch",
		"start": "serve dist",
		"prepare": "husky install"
	},
	"unused_scripts": {
		"build": "cross-env NODE_ENV=production npm-run-all clear:* request:* build:*",
		"build:eleventy": "eleventy",
		"build:webpack": "webpack",
		"build:pwa": "node pwa-assets.js",
		"build:workbox": "workbox generateSW workbox.config.js",
		"request:load": "node ./src/_requests/load.js",
		"request:profiles": "node ./src/_requests/profiles.js",
		"clear:basis": "rm -rf dist && rm -rf ./src/_data/*.json",
		"clear:js": "rm -rf ./src/_data/repos.js && rm -rf ./src/_data/articles.js && rm -rf ./src/_data/projects.js && rm -rf ./src/_data/sides.js",
		"dev": "cross-env NODE_ENV=development  npm-run-all clear:* request:* build:eleventy build:webpack -p dev:*",
		"dev:eleventy": "eleventy --serve --quiet",
		"dev:webpack": "webpack --watch",
		"start": "serve dist",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/devbaraus/portfolio_eleventy.git"
	},
	"keywords": [
		"eleventy",
		"tailwind",
		"workbox",
		"pwa",
		"graphql",
		"webpack",
		"husky"
	],
	"author": "Bruno de Araujo Alves",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/devbaraus/portfolio_eleventy/issues"
	},
	"homepage": "https://github.com/mattwaler/tea-stack#readme",
	"dependencies": {
		"@glidejs/glide": "^3.4.1",
		"axios": "^0.21.1",
		"graphql": "^15.5.0",
		"json-stringify-safe": "^5.0.1",
		"remove": "^0.1.5",
		"scrollreveal": "^4.0.9",
		"serve": "^14.1.0",
		"sharp-cli": "^1.15.0",
		"webfonts-loader": "^7.2.0",
		"workbox-cli": "^6.1.2"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.11.0",
		"@tailwindcss/typography": "^0.5.0",
		"autoprefixer": "^10.1.0",
		"cross-env": "^7.0.2",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.3.0",
		"date-fns": "^2.19.0",
		"dotenv": "^8.2.0",
		"graphql-request": "^3.4.0",
		"highlight.js": "^10.7.1",
		"highlightjs": "^9.16.2",
		"html-minifier": "^4.0.0",
		"json-stringify-pretty-compact": "^3.0.0",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^1.3.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.1",
		"postcss-import": "^14.0.0",
		"postcss-loader": "^4.1.0",
		"postcss-nested": "^5.0.5",
		"prettier": "^2.2.1",
		"showdown": "^1.9.1",
		"showdown-highlight-pre": "^2.1.5",
		"style-loader": "^2.0.0",
		"tailwindcss": "3.0.11",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.2.0",
		"husky": "^7.0.0"
	}
}