{
	"name": "tea-stack",
	"version": "1.0.0",
	"description": "A starter template for the TEA stack.",
	"engines": {
		"node": ">=12.19.0"
	},
	"main": "index.js",
	"scripts": {
		"build": "cross-env NODE_ENV=production npm-run-all clean request:* build:*",
		"build:eleventy": "eleventy",
		"build:webpack": "webpack",
		"build:pwa": "node pwa-assets.js",
		"build:workbox": "workbox generateSW workbox.config.js",
		"request:articles": "node ./src/_requests/articles.js",
		"request:repos": "node ./src/_requests/repos.js",
		"request:sides": "node ./src/_requests/sides.js",
		"request:projects": "node ./src/_requests/projects.js",
		"request:profiles": "node ./src/_requests/profiles.js",
		"clean": "rm -rf dist",
		"dev": "cross-env NODE_ENV=development  npm-run-all clean request:* build:eleventy build:webpack -p dev:*",
		"dev:eleventy": "eleventy --serve --quiet",
		"dev:webpack": "webpack --watch",
		"start": "serve dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mattwaler/tea-stack.git"
	},
	"keywords": [
		"eleventy",
		"tailwind",
		"alpine"
	],
	"author": "Matt Waler",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mattwaler/tea-stack/issues"
	},
	"homepage": "https://github.com/mattwaler/tea-stack#readme",
	"dependencies": {
		"@glidejs/glide": "^3.4.1",
		"axios": "^0.21.1",
		"graphql": "^15.5.0",
		"json-stringify-safe": "^5.0.1",
		"remove": "^0.1.5",
		"scrollreveal": "^4.0.9",
		"serve": "^12.0.0",
		"sharp-cli": "^1.15.0",
		"webfonts-loader": "^7.2.0",
		"workbox-cli": "^6.1.2"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.11.0",
		"@tailwindcss/typography": "^0.4.0",
		"autoprefixer": "^10.1.0",
		"cross-env": "^7.0.2",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.3.0",
		"date-fns": "^2.19.0",
		"dotenv": "^8.2.0",
		"graphql-request": "^3.4.0",
		"highlight.js": "^10.7.1",
		"highlightjs": "^9.16.2",
		"html-minifier": "^4.0.0",
		"json-stringify-pretty-compact": "^3.0.0",
		"lodash": "^4.17.21",
		"mini-css-extract-plugin": "^1.3.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.1",
		"postcss-import": "^14.0.0",
		"postcss-loader": "^4.1.0",
		"postcss-nested": "^5.0.5",
		"prettier": "^2.2.1",
		"showdown": "^1.9.1",
		"showdown-highlight-pre": "^2.1.5",
		"style-loader": "^2.0.0",
		"tailwindcss": "^2.0.2",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.2.0"
	}
}
